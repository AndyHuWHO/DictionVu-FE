{
  "expo": {
    "name": "Dictionvu",
    "slug": "Dictionvu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dictionvu-logo2.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "dictionvu",
    "splash": {
      "image": "./assets/dictionvu-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
